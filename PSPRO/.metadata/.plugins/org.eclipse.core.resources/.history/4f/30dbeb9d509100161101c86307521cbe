
public class RunThread {
	
	
	

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int pos = 0;
		int limite = 8;
		int[] numeros = new int[8];
		int siguiente = 0;
		
		numeros[pos] = pos;
		while (pos < limite) {
			
			
			pos++;
			numeros[pos] = pos + numeros[pos-1];
				
		}
		
		for (int i = 0; i < numeros.length; i++) {
			
			System.out.print(numeros[pos]+", ");
			
		}
		
		
		

	}

}
