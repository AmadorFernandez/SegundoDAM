
public class RunThread {
	
	
	

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		CalFibonaci calFibonaci = new CalFibonaci(10);
		calFibonaci.start();
		
		
		

	}

}

class CalFibonaci extends Thread{
	
	int n;
	
	
	public CalFibonaci(int n){
		
		this.n = n;
		
	}
	
	@Override
	public void run() {
		
		int fibo1 = 1;
		int fibo2 = 1;
		
		
		for(int i = 0; i <= n; i++){
			
			System.out.print(fibo2+", ");
			fibo2 = fibo1 + fibo2;
			fibo1 = fibo2 - fibo1;	
		
	}
	
	
}
