
public class RunThread {
	
	
	

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int numero = 0;
		int limite = 8;
		int[] numeros = new int[8];
		
		for (int i = 0; i <= numeros.length; i++) {
			
			numeros[i] = (i-1) + (i - 2);
			System.out.print(numeros[i]+", ");
			
		}
		
		
		

	}

}
