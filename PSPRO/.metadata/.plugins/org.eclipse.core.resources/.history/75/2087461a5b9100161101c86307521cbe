
public class RunThread {
	
    static long sucesion = 0;
	

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		CalFibonaci calFibonaci = new CalFibonaci(10);
		calFibonaci.start();
		
		try {
		//	calFibonaci.join();
			System.out.print(sucesion+", ");
		} catch (InterruptedException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		
		
		
		

	}

}

class CalFibonaci extends Thread{
	
    private	int n;
    private int fiboNumero;
    
    public int getFiboNumero(){
    	
    	return fiboNumero;
    	
    }

    public void setFiboNumero(int value){
    	
    	fiboNumero = value;
    }
	
	
	public CalFibonaci(int n){
		
		this.n = n;
		this.fiboNumero = 1;
		
	}
	
	@Override
	public void run() {
		
	
		
		
		
		int fibo1 = 1;
		int fibo2 = 1;
		
		
		for(int i = 0; i < n; i++){
			
		//	System.out.print(fibo2+", ");
			fibo2 = fibo1 + fibo2;
			fibo1 = fibo2 - fibo1;	
			RunThread.sucesion = fibo2;
		
		}
	
	
	
	}
	
}
