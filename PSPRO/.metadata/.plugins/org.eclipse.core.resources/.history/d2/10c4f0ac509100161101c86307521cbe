
public class RunThread {
	
	
	

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int pos = 0;
		int limite = 8;
		int[] numeros = new int[8];
		int siguiente = 0;
		
	
		numeros[0] = 0;
		
		for (int i = 1; i < numeros.length; i++) {
			
			numeros[i] = numeros[i-1];
			System.out.print(numeros[i]+", ");
			
		}
		
		
		

	}

}
